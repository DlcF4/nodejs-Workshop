include ./includes/header.jade

h1=title
table(class='table table-striped table-hover table-condensed')
  thead
    tr
      th Job
      th User
      th Number
      th(style="text-align:right") Spoolfiles
      th(style="text-align:right") Spoolfiles size
      th Newest spoolfile
  tbody
    - each row in results
      tr
        td=row.JOBNAME
        td=row.JOBUSER
        td=row.JOBNBR
        td(style="text-align:right").utility #{_.str.numberFormat(Number(row.SPLF_COUNT), 0, ",", ".")}
        td(style="text-align:right").utility #{_.str.numberFormat(Number(row.SPLF_SIZE), 0, ",", ".")}
        td=row.NEWEST_SPLF
          
include ./includes/footer.jade