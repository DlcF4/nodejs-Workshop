include ./includes/header.jade

h1=title
table(class='table table-striped table-hover table-condensed')
  thead
    tr
      th Name
      th Status
      th Description
      th Storage Used
  tbody
    - each row in results
      tr
        td: a(href='/user/#{row.AUTHORIZATION_NAME}')=row.AUTHORIZATION_NAME 
        td=row.STATUS
        td=row.TEXT_DESCRIPTION 
        td=row.STORAGE_USED
          
include ./includes/footer.jade